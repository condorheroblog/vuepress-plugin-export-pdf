    "packages/vuepress-plugin-export-pdf-v1/example/vue-press",
    "packages/vuepress-plugin-export-pdf-v2/example/vuepress-next"
  ],
  "devDependencies": {
    "@antfu/eslint-config": "^2.17.0",
    "bumpp": "^9.4.1",
    "eslint": "^9.2.0",
    "esno": "^4.7.0",
    "fast-glob": "^3.3.2",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "pnpm": "^9.1.0",
    "typescript": "^5.4.5",
    "unbuild": "^2.0.0",
